{
    "name": "antikode/anti-cms-builder",
    "description": "A powerful Laravel package for building dynamic CRUD interfaces with minimal boilerplate code. Provides form builders, table builders, and React components to accelerate development.",
    "keywords": ["laravel", "crud", "form-builder", "table-builder", "cms", "admin-panel", "react", "inertia"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/antikode/anti-cms-builder",
    "support": {
        "issues": "https://github.com/antikode/anti-cms-builder/issues",
        "source": "https://github.com/antikode/anti-cms-builder"
    },
    "authors": [
        {
            "name": "Antikode",
            "email": "admin@antikode.com",
            "homepage": "https://antikode.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AntiCmsBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntiCmsBuilder\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntiCmsBuilder\\AntiCmsBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
